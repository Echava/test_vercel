{
  "version": 2,
  "framework": "astro",
  "buildCommand": "astro build",
  "devCommand": "astro dev",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}